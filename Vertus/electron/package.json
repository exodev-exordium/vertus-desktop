{
  "name": "vertus-desktop",
  "version": "1.0.0",
  "productName": "Vertus",
  "description": "An open source terminal, ssh, and sftp-client for desktop platforms, made with electronjs, ionic, and capacitor. ",
  "main": "index.js",
  "scripts": {
    "electron:start": "electron ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/exordium-dev/vertus-desktop/exordium-desktop.git"
  },
  "author": {
    "name": "FearGannicus",
    "email": "gannicus@exordium.dev",
    "url": "https://exordium.dev"
  },
  "bugs": {
    "url": "https://github.com/exordium-dev/vertus-desktop/issues"
  },
  "build": {
    "appID": "dev.exordium.vertus",
    "productName": "Vertus by Exordium",
    "copyright": "Exordium 2020",
    "asar": true,
    "publish": [
      {
        "provider": "github",
        "owner": "exordium-dev",
        "repo": "vertus-desktop"
      }
    ],
    "win": {
      "target": [
        "nsis",
        "zip"
      ],
      "icon": "assets/icons/ ..."
    }
  },
  "dependencies": {
    "@capacitor/electron": "^1.5.1",
    "discord-rich-presence": "0.0.8",
    "electron-is-dev": "^1.1.0",
    "electron-log": "^4.0.7",
    "electron-updater": "^2.2.4"
  },
  "devDependencies": {
    "electron": "^9.4.0"
  },
  "keywords": [
    "capacitor",
    "electron",
    "vertus",
    "ssh",
    "terminal",
    "cmd",
    "sftp",
    "client"
  ],
  "license": "ISC"
}
